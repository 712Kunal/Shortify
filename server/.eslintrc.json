{
    "env": {
      "node": true,
      "es2021": true
    },
    "extends": [
      "standard",
      "plugin:@typescript-eslint/recommended"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
    },
    "plugins": ["@typescript-eslint"],
    "rules": {
      "no-useless-escape": "off",
      "indent": ["error", 4],
      "no-console": "off",  
      "@typescript-eslint/no-unused-vars": "off",  
      "consistent-return": "off"  
    }
  }
  